<?xml version="1.0" ?>
<robot name="table">
  <!-- Define a world link -->
  <link name="world"/>
  
  <!-- Define the base link (tabletop) -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="1.5 0.8 0.1"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.4"/>
    </visual>
  </link>

  <!-- Fixed joint to link base_link to world -->
  <joint name="fixed_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.4"/>
  </joint>

  <!-- Table Legs (Adjusted to Connect Properly) -->
  <!-- Leg 1 (Front Left) -->
  <link name="leg_1">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.7"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-0.7 -0.35 0"/>
    </visual>
  </link>
  <joint name="joint_leg_1" type="fixed">
    <parent link="base_link"/>
    <child link="leg_1"/>
    <origin rpy="0 0 0" xyz="-0 0 0"/>
  </joint>

  <!-- Leg 2 (Front Right) -->
  <link name="leg_2">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.7"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.7 -0.35 0"/>
    </visual>
  </link>
  <joint name="joint_leg_2" type="fixed">
    <parent link="base_link"/>
    <child link="leg_2"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <!-- Leg 3 (Back Left) -->
  <link name="leg_3">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.7"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-0.7 0.35 0"/>
    </visual>
  </link>
  <joint name="joint_leg_3" type="fixed">
    <parent link="base_link"/>
    <child link="leg_3"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <!-- Leg 4 (Back Right) -->
  <link name="leg_4">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.7"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.7 0.35 0"/>
    </visual>
  </link>
  <joint name="joint_leg_4" type="fixed">
    <parent link="base_link"/>
    <child link="leg_4"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

</robot>
